<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiStateView">
        <attr name="loadingView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="defaultViewState" format="enum">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="empty" value="2" />
            <enum name="loading" value="3" />
        </attr>
        <attr name="emptyIcon" format="reference" />
        <attr name="emptyText" format="reference" />
        <attr name="errorIcon" format="reference" />
        <attr name="errorText" format="reference" />
    </declare-styleable>
</resources>